      </wizardPage>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.n52.udig.catalog.internal.sos.ui.SOSPreferencePage1"
            id="org.n52.udig.catalog.sos.page2"
            name="SOSPreferences">
      </page>
